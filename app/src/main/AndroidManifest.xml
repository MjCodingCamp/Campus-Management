<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mjsiddiqui.campusmanagement">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="Activity.ChatActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name="Activity.NoticeActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name="Activity.MarksDisplayActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.AttendanceDisplayActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.FailedActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.SuccessActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.PdfViewerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.ProfileActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.HomeActivity2"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.SignUpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="Activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>